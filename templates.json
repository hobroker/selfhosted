{
  "version": "2",
  "templates": [
    {
      "type": 2,
      "name": "jackett",
      "repository": {
        "url": "https://github.com/hobroker/nas-setup.git",
        "stackfile": "stacks/jackett/docker-stack.yml"
      },
      "title": "Jackett",
      "description": "API Support for your favorite torrent trackers",
      "logo": "https://raw.githubusercontent.com/Jackett/Jackett/master/src/Jackett.Common/Content/apple-touch-icon.png",
      "categories": [
        "media"
      ],
      "env": [
        {
          "name": "PUID",
          "label": "UserID",
          "default": "1000"
        },
        {
          "name": "PGID",
          "label": "GroupID",
          "default": "1000"
        },
        {
          "name": "TZ",
          "label": "Timezone",
          "default": "Europe/Chisinau"
        },
        {
          "name": "CONFIG",
          "label": "Config path",
          "default": "/appdata/jackett"
        },
        {
          "name": "BLACKHOLE",
          "label": "Blackhole path"
        },
        {
          "name": "PORT",
          "label": "WebUI Port",
          "default": "9117"
        }
      ]
    },
    {
      "type": 2,
      "name": "nginx-proxy-manager",
      "repository": {
        "url": "https://github.com/hobroker/nas-setup.git",
        "stackfile": "stacks/nginx-proxy-manager/docker-stack.yml"
      },
      "title": "Nginx Proxy Manager",
      "description": "Container for managing Nginx proxy hosts with a simple, powerful interface",
      "logo": "https://raw.githubusercontent.com/jc21/nginx-proxy-manager/master/frontend/app-images/logo-256.png",
      "categories": [
        "network"
      ],
      "env": [
        {
          "name": "CONFIG_JSON",
          "label": "JSON Config",
          "default": "/appdata/nginx-proxy-manager/config.json"
        },
        {
          "name": "DB",
          "label": "Database",
          "default": "/appdata/nginx-proxy-manager/npm.db"
        },
        {
          "name": "DATA",
          "label": "App data",
          "default": "/appdata/nginx-proxy-manager/data"
        },
        {
          "name": "LETSENCRYPT",
          "label": "Let's Encrypt data",
          "default": "/appdata/nginx-proxy-manager/letsencrypt"
        }
      ]
    },
    {
      "type": 2,
      "name": "portainer",
      "repository": {
        "url": "https://github.com/hobroker/nas-setup.git",
        "stackfile": "stacks/portainer/docker-stack.yml"
      },
      "title": "Portainer",
      "description": "Management UI to easily manage different Docker environments",
      "logo": "https://raw.githubusercontent.com/portainer/portainer/develop/app/assets/ico/apple-touch-icon.png",
      "categories": [
        "docker",
        "network"
      ],
      "env": [
        {
          "name": "PORT",
          "label": "WebUI Port",
          "default": "9000"
        },
        {
          "name": "EDGE_PORT",
          "label": "Edge Agent Port",
          "default": "8000"
        },
        {
          "name": "DOCKER_SOCKET",
          "label": "Docker socket path",
          "default": "/var/run/docker.sock"
        },
        {
          "name": "CONFIG",
          "label": "Config path",
          "default": "/appdata/portainer"
        }
      ]
    },
    {
      "type": 2,
      "name": "qbittorrent",
      "repository": {
        "url": "https://github.com/hobroker/nas-setup.git",
        "stackfile": "stacks/qbittorrent/docker-stack.yml"
      },
      "title": "qBittorrent",
      "description": "BitTorrent client",
      "logo": "https://raw.githubusercontent.com/qbittorrent/qBittorrent/master/dist/unix/menuicons/128x128/apps/qbittorrent.png",
      "categories": [
        "torrent"
      ],
      "env": [
        {
          "name": "PUID",
          "label": "UserID",
          "default": "1000"
        },
        {
          "name": "PGID",
          "label": "GroupID",
          "default": "1000"
        },
        {
          "name": "TZ",
          "label": "Timezone",
          "default": "Europe/Chisinau"
        },
        {
          "name": "CONFIG_PATH",
          "label": "App data",
          "default": "/appdata/qbittorrent"
        },
        {
          "name": "DOWNLOADS_PATH",
          "label": "Downloads path",
          "default": "/mnt/storage/downloads"
        },
        {
          "name": "TCP_PORT",
          "label": "TCP connection port",
          "default": "6881"
        },
        {
          "name": "UDP_PORT",
          "label": "UDP connection port",
          "default": "6881"
        }
      ]
    },
    {
      "type": 2,
      "name": "radarr",
      "repository": {
        "url": "https://github.com/hobroker/nas-setup.git",
        "stackfile": "stacks/radarr/docker-stack.yml"
      },
      "title": "Radarr",
      "description": "Movie collection manager for Usenet and BitTorrent users",
      "logo": "https://raw.githubusercontent.com/Radarr/Radarr/develop/Logo/64.png",
      "categories": [
        "media"
      ],
      "env": [
        {
          "name": "PUID",
          "label": "UserID",
          "default": "1000"
        },
        {
          "name": "PGID",
          "label": "GroupID",
          "default": "1000"
        },
        {
          "name": "TZ",
          "label": "Timezone",
          "default": "Europe/Chisinau"
        },
        {
          "name": "7878",
          "label": "WebUI Port",
          "default": "7878"
        }
      ]
    },
    {
      "type": 2,
      "name": "sonarr",
      "repository": {
        "url": "https://github.com/hobroker/nas-setup.git",
        "stackfile": "stacks/sonarr/docker-stack.yml"
      },
      "title": "Sonarr",
      "description": "TV Shows collection manager for Usenet and BitTorrent users",
      "logo": "https://raw.githubusercontent.com/Sonarr/Sonarr/phantom-develop/Logo/256.png",
      "categories": [
        "media"
      ],
      "env": [
        {
          "name": "PUID",
          "label": "UserID",
          "default": "1000"
        },
        {
          "name": "PGID",
          "label": "GroupID",
          "default": "1000"
        },
        {
          "name": "TZ",
          "label": "Timezone",
          "default": "Europe/Chisinau"
        },
        {
          "name": "CONFIG_PATH",
          "label": "App data",
          "default": "/appdata/sonarr"
        },
        {
          "name": "TV_PATH",
          "label": "The TV Shows location",
          "default": "/mnt/storage/tv-shows"
        },
        {
          "name": "DOWNLOADS_PATH",
          "label": "The downloads location",
          "default": "/mnt/storage/downloads"
        },
        {
          "name": "PORT",
          "label": "WebUI Port",
          "default": "8989"
        }
      ]
    },
    {
      "type": 2,
      "name": "tautulli",
      "repository": {
        "url": "https://github.com/hobroker/nas-setup.git",
        "stackfile": "stacks/tautulli/docker-stack.yml"
      },
      "title": "Tautulli",
      "description": "Monitoring and tracking tool for Plex Media Server",
      "logo": "https://raw.githubusercontent.com/Tautulli/Tautulli/master/data/interfaces/default/images/logo-circle.png",
      "categories": [
        "media"
      ],
      "env": [
        {
          "name": "PUID",
          "label": "UserID",
          "default": "1000"
        },
        {
          "name": "PGID",
          "label": "GroupID",
          "default": "1000"
        },
        {
          "name": "TZ",
          "label": "Timezone",
          "default": "Europe/Chisinau"
        },
        {
          "name": "CONFIG_PATH",
          "label": "Contains Tautulli config and database"
        },
        {
          "name": "PORT",
          "label": "WebUI Port",
          "default": "8181"
        }
      ]
    },
    {
      "type": 2,
      "name": "xteve",
      "repository": {
        "url": "https://github.com/hobroker/nas-setup.git",
        "stackfile": "stacks/xteve/docker-stack.yml"
      },
      "title": "xTeVe",
      "description": "M3U Proxy for Plex DVR and Emby Live TV",
      "logo": "https://raw.githubusercontent.com/xteve-project/xTeVe/master/html/img/x_black.png",
      "categories": [
        "media",
        "iptv"
      ],
      "env": [
        {
          "name": "34400",
          "label": "Exposed port",
          "default": "34400"
        },
        {
          "name": "TZ",
          "label": "Timezone",
          "default": "Europe/Chisinau"
        },
        {
          "name": "ROOT_PATH",
          "label": "Root path",
          "default": "/appdata/xteve/root"
        },
        {
          "name": "CONFIG_PATH",
          "label": "Config path",
          "default": "/appdata/xteve/_config/"
        },
        {
          "name": "GUIDE2GO_PATH",
          "label": "Guide2go path",
          "default": "/appdata/xteve/_guide2go/"
        },
        {
          "name": "OWI2PLEX_PATH",
          "label": "Owi2plex path",
          "default": "/appdata/xteve/_owi2plex/"
        },
        {
          "name": "TVHEADEND_PATH",
          "label": "Tvheadend path",
          "default": "/appdata/xteve/tvheadend/data/"
        },
        {
          "name": "TMP_PATH",
          "label": "tmp path",
          "default": "/tmp/xteve/"
        }
      ]
    }
  ]
}
