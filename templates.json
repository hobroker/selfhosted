{
  "version": "2",
  "templates": [
    {
      "name": "jackett",
      "type": 2,
      "title": "Jackett",
      "description": "API Support for your favorite torrent trackers",
      "logo": "https://raw.githubusercontent.com/Jackett/Jackett/master/src/Jackett.Common/Content/apple-touch-icon.png",
      "categories": [
        "media"
      ],
      "repository": {
        "url": "https://github.com/hobroker/selfhosted.git",
        "stackfile": "stacks/single/jackett/docker-stack.yml"
      },
      "env": [
        {
          "name": "PUID",
          "label": "UserID",
          "default": "1000"
        },
        {
          "name": "PGID",
          "label": "GroupID",
          "default": "1000"
        },
        {
          "name": "TZ",
          "label": "Timezone",
          "default": "Europe/Chisinau"
        },
        {
          "name": "CONFIG",
          "label": "Config path",
          "default": "/appdata/jackett"
        },
        {
          "name": "BLACKHOLE",
          "label": "Blackhole path",
          "default": "/storage/downloads/torrents/"
        },
        {
          "name": "PORT",
          "label": "WebUI Port",
          "default": "9117"
        }
      ]
    },
    {
      "name": "nginx-proxy-manager",
      "type": 2,
      "title": "Nginx Proxy Manager",
      "description": "Container for managing Nginx proxy hosts with a simple, powerful interface",
      "logo": "https://raw.githubusercontent.com/jc21/nginx-proxy-manager/master/frontend/app-images/logo-256.png",
      "categories": [
        "network"
      ],
      "repository": {
        "url": "https://github.com/hobroker/selfhosted.git",
        "stackfile": "stacks/single/nginx-proxy-manager/docker-stack.yml"
      },
      "env": [
        {
          "name": "DISABLE_IPV6",
          "label": "Disable IPv6",
          "default": "true"
        },
        {
          "name": "CONFIG_JSON",
          "label": "JSON Config",
          "default": "/appdata/nginx-proxy-manager/config.json"
        },
        {
          "name": "DB",
          "label": "Database",
          "default": "/appdata/nginx-proxy-manager/npm.db"
        },
        {
          "name": "DATA",
          "label": "Appdata",
          "default": "/appdata/nginx-proxy-manager/data"
        },
        {
          "name": "LETSENCRYPT",
          "label": "Let's Encrypt data",
          "default": "/appdata/nginx-proxy-manager/letsencrypt"
        }
      ]
    },
    {
      "name": "plex",
      "type": 2,
      "title": "Plex",
      "description": "Media player and organizational tool for accessing videos, music and pictures",
      "logo": "https://raw.githubusercontent.com/plexinc/plex-media-player/master/resources/images/icon.png",
      "categories": [
        "media"
      ],
      "repository": {
        "url": "https://github.com/hobroker/selfhosted.git",
        "stackfile": "stacks/single/plex/docker-stack.yml"
      },
      "env": [
        {
          "name": "PUID",
          "label": "UserID",
          "default": "1000"
        },
        {
          "name": "PGID",
          "label": "GroupID",
          "default": "1000"
        },
        {
          "name": "UMASK_SET",
          "label": "Plex file-creation mode",
          "default": "022"
        },
        {
          "name": "APPDATA",
          "label": "Plex appdata location",
          "default": "/appdata/plex"
        },
        {
          "name": "STORAGE",
          "label": "Media library location 1",
          "default": "/mnt/storage"
        },
        {
          "name": "STORAGE",
          "label": "Media library location 2",
          "default": "/storage"
        }
      ]
    },
    {
      "name": "pomerium",
      "type": 2,
      "title": "Pomerium",
      "description": "Identity-aware access proxy that enables secure access to internal applications",
      "logo": "https://raw.githubusercontent.com/pomerium/pomerium/master/docs/.vuepress/public/img/logo-round.png",
      "categories": [
        "network"
      ],
      "repository": {
        "url": "https://github.com/hobroker/selfhosted.git",
        "stackfile": "stacks/single/pomerium/docker-stack.yml"
      },
      "env": [
        {
          "name": "AUTHENTICATE_SERVICE_URL",
          "label": "Secret key",
          "default": "https://auth.hobroker.me"
        },
        {
          "name": "IDP_CLIENT_ID",
          "label": " Google Client ID"
        },
        {
          "name": "IDP_CLIENT_SECRET",
          "label": " Google Client Secret"
        },
        {
          "name": "POLICY",
          "label": "Policy (base64)"
        }
      ]
    },
    {
      "name": "portainer",
      "type": 2,
      "title": "Portainer",
      "description": "Management UI to easily manage different Docker environments",
      "logo": "https://raw.githubusercontent.com/portainer/portainer/develop/app/assets/ico/apple-touch-icon.png",
      "categories": [
        "docker",
        "portainer"
      ],
      "repository": {
        "url": "https://github.com/hobroker/selfhosted.git",
        "stackfile": "stacks/single/portainer/docker-stack.yml"
      },
      "env": [
        {
          "name": "PORT",
          "label": "WebUI Port",
          "default": "9000"
        },
        {
          "name": "EDGE_PORT",
          "label": "Edge Agent Port",
          "default": "8000"
        },
        {
          "name": "CONFIG",
          "label": "Config path",
          "default": "/appdata/portainer"
        }
      ]
    },
