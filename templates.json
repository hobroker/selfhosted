{
  "version": "2",
  "templates": [
    {
      "type": 2,
      "name": "jackett",
      "repository": {
        "url": "https://github.com/hobroker/nas-setup.git",
        "stackfile": "stacks/jackett/docker-stack.yml"
      },
      "title": "Jackett",
      "description": "API Support for your favorite torrent trackers",
      "logo": "https://raw.githubusercontent.com/Jackett/Jackett/master/src/Jackett.Common/Content/apple-touch-icon.png",
      "categories": [
        "media"
      ],
      "env": [
        {
          "name": "PUID",
          "label": "UserID",
          "default": "1000"
        },
        {
          "name": "PGID",
          "label": "GroupID",
          "default": "1000"
        },
        {
          "name": "TZ",
          "label": "Timezone",
          "default": "Europe/Chisinau"
        },
        {
          "name": "CONFIG",
          "label": "Config path",
          "default": "/appdata/jackett"
        },
        {
          "name": "BLACKHOLE",
          "label": "Blackhole path"
        },
        {
          "name": "PORT",
          "label": "WebUI Port",
          "default": "9117"
        }
      ]
    },
    {
      "type": 2,
      "name": "nginx-proxy-manager",
      "repository": {
        "url": "https://github.com/hobroker/nas-setup.git",
        "stackfile": "stacks/nginx-proxy-manager/docker-stack.yml"
      },
      "title": "Nginx Proxy Manager",
      "description": "Container for managing Nginx proxy hosts with a simple, powerful interface",
      "logo": "https://raw.githubusercontent.com/jc21/nginx-proxy-manager/master/frontend/app-images/logo-256.png",
      "categories": [
        "network"
      ],
      "env": [
        {
          "name": "HTTP_PORT",
          "label": "HTTP Port",
          "default": "80"
        },
        {
          "name": "HTTPS_PORT",
          "label": "HTTPS Port",
          "default": "443"
        },
        {
          "name": "UI_PORT",
          "label": "WebUI Port",
          "default": "81"
        },
        {
          "name": "CONFIG_JSON",
          "label": "JSON Config",
          "default": "/appdata/nginx-proxy-manager/config.json"
        },
        {
          "name": "DB",
          "label": "Database",
          "default": "/appdata/nginx-proxy-manager/npm.db"
        },
        {
          "name": "DATA",
          "label": "App data",
          "default": "/appdata/nginx-proxy-manager/data"
        },
        {
          "name": "LETSENCRYPT",
          "label": "Let's Encrypt data",
          "default": "/appdata/nginx-proxy-manager/letsencrypt"
        }
      ]
    },
    {
      "type": 2,
      "name": "portainer",
      "repository": {
        "url": "https://github.com/hobroker/nas-setup.git",
        "stackfile": "stacks/portainer/docker-stack.yml"
      },
      "title": "Portainer",
      "description": "Management UI to easily manage different Docker environments",
      "logo": "https://raw.githubusercontent.com/portainer/portainer/develop/app/assets/ico/apple-touch-icon.png",
      "categories": [
        "docker",
        "network"
      ],
      "env": [
        {
          "name": "PORT",
          "label": "WebUI Port",
          "default": "9000"
        },
        {
          "name": "EDGE_PORT",
          "label": "Edge Agent Port",
          "default": "8000"
        },
        {
          "name": "DOCKER_SOCKET",
          "label": "Docker socket path",
          "default": "/var/run/docker.sock"
        },
        {
          "name": "CONFIG",
          "label": "Config path",
          "default": "/appdata/portainer"
        }
      ]
    },
    {
      "type": 2,
      "name": "tautulli",
      "repository": {
        "url": "https://github.com/hobroker/nas-setup.git",
        "stackfile": "stacks/tautulli/docker-stack.yml"
      },
      "title": "Tautulli",
      "description": "Monitoring and tracking tool for Plex Media Server",
      "logo": "https://raw.githubusercontent.com/Tautulli/Tautulli/master/data/interfaces/default/images/logo-circle.png",
      "categories": [
        "media"
      ],
      "env": [
        {
          "name": "PUID",
          "label": "UserID",
          "default": "1000"
        },
        {
          "name": "PGID",
          "label": "GroupID",
          "default": "1000"
        },
        {
          "name": "TZ",
          "label": "Timezone",
          "default": "Europe/Chisinau"
        },
        {
          "name": "CONFIG_PATH",
          "label": "Contains Tautulli config and database"
        },
        {
          "name": "PORT",
          "label": "WebUI Port",
          "default": "8181"
        }
      ]
    }
  ]
}
